class Double2
{
	public static void main(String[] args) 
	{
		double ae=128;
        byte a=(byte)ae;
		System.out.println("byte:"+a);
		double Wq=32768;
		short h=(short)Wq;
		System.out.println("shot:" +h);
		double hg=2147448;
		int i=(int)hg;
		System.out.println("int:" +i);
		double kj=884367;
		long tre=(long)kj;
		System.out.println("long:" +tre);
		double d=8787976;
		float k=(float)d;
		System.out.println("float:" +k);
        double e=99;
		char ch=e;
		System.out.println("char"+ch);
	}
}
		