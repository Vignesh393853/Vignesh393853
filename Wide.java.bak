class WideB
{
	public static void main(String[] args) 
	{
		byte a=9;
		short h=a;
		System.out.println("shot:" +h);
		byte b=127;
		int i=b;
		System.out.println("int:" +i);
		byte c=73;
		long j=c;
		System.out.println("long:" +j);
		byte d=87;
		float k=d;
		System.out.println("float:" +k);
		byte e=98;
		double l=e;
		System.out.println("double:" +l);
		short a=66;
		char ch=a;
		System.out.println("char"+ch);
		
	}
}
